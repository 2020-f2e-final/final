<h1>maps</h1>

<div class="w-11/12 sm:w-4/5 md:w-2/3 lg:4/5 xl:max-w-screen-xl mx-auto">
    <div class="hidden lg:block">
        <div class="flex justify-around items-center mt-20 mb-36">
            <% data.mapStars.forEach((s, i) => { %>
                <div>
                    <div class="flex justify-center text-3xl text-main-map-diff "><%= s.diff %></div>
                    <div class="flex justify-center text-2xl text-main-map-star"><%= s.star %></div>
                </div>
                <% }) %>
        </div>
        <div><%- include('all') %></div>
    </div>
    <div class="lg:hidden">
        not yet...
    </div>
</div>